{
  "name": "aurora-konsoll",
  "groupId": "no.skatteetaten.aurora",
  "version": "0.1.1",
  "private": false,
  "author": {
    "name": "Skatteetaten"
  },
  "files": [
    "build",
    "build-node/api",
    "metadata",
    "git-properties.json"
  ],
  "bundledDependencies": [
    "@skatteetaten/aurora-management-interface",
    "http-proxy-middleware",
    "express",
    "winston",
    "winston-daily-rotate-file"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "1.5.1",
    "@skatteetaten/aurora-management-interface": "1.0.3",
    "@skatteetaten/frontend-components": "4.1.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "graphql-tag": "2.12.4",
    "http-proxy-middleware": "2.0.0",
    "immer": "9.0.2",
    "isomorphic-fetch": "3.0.0",
    "lodash": "4.17.21",
    "luxon": "1.27.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-select": "4.3.1",
    "redux-thunk": "2.3.0",
    "redux-ts-utils": "4.0.0",
    "single-spa-react": "4.1.1",
    "styled-components": "5.3.0",
    "systemjs-webpack-interop": "2.3.6",
    "typesafe-actions": "5.1.0",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.5",
    "yaml": "1.10.2"
  },
  "scripts": {
    "prestart": "npm run clean",
    "start": "npm-run-all -lp build:api:watch start:api start:web",
    "start:api": "nodemon",
    "start:web": "react-scripts start",
    "prebuild": "npm-run-all clean lint && aurora-mi git",
    "build": "GENERATE_SOURCEMAP=false npm-run-all -lp build:api build:web:spa",
    "build:api": "tsc --build tsconfig.api.json",
    "build:api:watch": "npm run build:api -- --watch --preserveWatchOutput",
    "build:web": "react-scripts build",
    "build:web:spa": "GENERATE_SOURCEMAP=false rescripts build",
    "start:spa": "npm-run-all -lp build:api:watch start:api start:spa:web",
    "start:spa:web": "rescripts start",
    "clean": "rimraf build build-node coverage pom *.tgz git-properties.json",
    "test": "react-scripts test --env=jsdom",
    "test:once": "CI=true npm test",
    "test:coverage": "CI=true npm run test -- --coverage --testResultsProcessor jest-junit",
    "lint": "eslint 'src/**/*{.ts,.tsx}'",
    "lint:fix": "eslint --fix 'src/**/*{.ts,.tsx}'",
    "eject": "react-scripts eject"
  },
  "nodemonConfig": {
    "ext": "*.js",
    "exec": "node build-node/setupDevelopment.js",
    "runOnChangeOnly": true,
    "watch": "build-node/**/*",
    "delay": "200"
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@rescripts/cli": "0.0.16",
    "@rescripts/rescript-env": "0.0.12",
    "@skatteetaten/ci-scripts": "2.0.5",
    "@types/cors": "2.8.10",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/express": "4.17.12",
    "@types/graphql": "14.5.0",
    "@types/http-proxy-middleware": "0.19.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/luxon": "1.26.5",
    "@types/node": "15.6.1",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/react-select": "4.0.15",
    "@types/styled-components": "5.1.9",
    "@types/uuid": "8.3.0",
    "dotenv": "10.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.23.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-sonarjs": "0.7.0",
    "factory.ts": "0.5.2",
    "jest-each": "27.0.2",
    "jest-junit": "12.1.0",
    "nodemon": "2.0.15",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "react-scripts": "4.0.3",
    "redux-devtools-extension": "2.13.9",
    "rimraf": "3.0.2",
    "ts-loader": "8.3.0",
    "typescript": "4.3.2",
    "typescript-styled-plugin": "0.17.0"
  },
  "jest-junit": {
    "suiteName": "Unit Tests",
    "outputDirectory": "coverage/surefire-reports",
    "outputName": "TEST-jest.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "proxy": "http://localhost:9090",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(@skatteetaten/frontend-components)"
    ]
  },
  "resolutions": {
    "**/jest-haste-map/fsevents": "2.1.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
