{
  "name": "aurora-konsoll",
  "groupId": "no.skatteetaten.aurora",
  "version": "0.1.1",
  "private": false,
  "author": {
    "name": "Skatteetaten"
  },
  "files": [
    "build",
    "build-api",
    "metadata"
  ],
  "dependencies": {
    "apollo-boost": "0.3.1",
    "aurora-frontend-react-komponenter": "1.4.0",
    "cors": "2.8.5",
    "express": "4.16.4",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.1",
    "http-proxy-middleware": "0.19.1",
    "isomorphic-fetch": "2.2.1",
    "office-ui-fabric-react": "6.114.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-konami-code": "1.4.0",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.0",
    "react-select": "2.4.3",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "redux-ts-utils": "3.2.2",
    "styled-components": "3.4.8",
    "typesafe-actions": "4.2.0",
    "uuid": "3.3.2"
  },
  "scripts": {
    "start": "npm-run-all -pl start:web start:api",
    "start:web": "react-scripts start",
    "start:api": "npm run build:api -- --watch & nodemon --watch ./build-api --exec \"node ./build-api/server.js\"",
    "build": "npm-run-all -pl build:web build:api",
    "build:web": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:api": "webpack --config webpack.api.config.js --mode production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf build build-api coverage pom *.tgz",
    "test:once": "CI=true npm test",
    "test:coverage": "npm run test -- --coverage",
    "prettier": "prettier -l '{api,src}/**/*{.ts,.tsx,.css}'",
    "prettier:fix": "prettier --write '{api,src}/**/*{.ts,.tsx,.css}'"
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@types/cors": "2.8.5",
    "@types/enzyme": "3.9.1",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/express": "4.16.1",
    "@types/graphql": "14.2.0",
    "@types/http-proxy-middleware": "0.19.2",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.8",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.0.3",
    "@types/react-router-dom": "4.3.2",
    "@types/react-select": "2.0.17",
    "@types/styled-components": "4.1.14",
    "@types/uuid": "3.4.4",
    "aurora-openshift": "0.0.12",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "factory.ts": "0.4.5",
    "jest-each": "24.7.1",
    "nodemon": "1.18.11",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.0",
    "rimraf": "2.6.3",
    "ts-loader": "5.4.4",
    "typescript": "3.4.5",
    "typescript-styled-plugin": "0.14.0",
    "webpack-cli": "3.3.1"
  },
  "proxy": "http://localhost:9090",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.d.ts",
      "!src/setupTests.ts",
      "!src/services/registerServiceWorker.ts"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
