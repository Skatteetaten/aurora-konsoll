    "name": "Skatteetaten"
  },
  "files": [
    "build",
    "build-api",
    "metadata"
  ],
  "dependencies": {
    "@skatteetaten/frontend-components": "3.1.1",
    "aurora-management-interface": "0.6.1",
    "cors": "2.8.5",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "http-proxy-middleware": "0.20.0",
    "immer": "4.0.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.15",
    "office-ui-fabric-react": "7.70.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-select": "3.0.8",
    "redux-thunk": "2.3.0",
    "redux-ts-utils": "3.2.2",
    "styled-components": "4.4.1",
    "typesafe-actions": "4.4.2",
    "uuid": "3.3.3",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "start:web": "react-scripts start",
    "start:api": "npm run build:api -- --watch & nodemon --watch ./build-api --exec \"node ./build-api/server.js\"",
    "prebuild": "npm-run-all clean lint",
    "build": "npm-run-all -pl build:web build:api",
    "build:web": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:api": "webpack --config webpack.api.config.js --mode production",
    "test": "react-scripts test",
    "test:api": "jest api/*.ts",
    "eject": "react-scripts eject",
    "clean": "rimraf build build-api coverage pom *.tgz",
    "test:once": "CI=true npm test",
    "test:web:coverage": "CI=true npm run test -- --coverage --testResultsProcessor jest-junit",
    "test:coverage": "CI=true npm-run-all test:web:coverage test:api",
    "prettier": "prettier -l '{api,src}/**/*{.ts,.tsx,.css}'",
    "prettier:fix": "prettier --write '{api,src}/**/*{.ts,.tsx,.css}'",
    "lint": "eslint '{api,src}/**/*{.ts,.tsx}'",
    "lint:fix": "eslint --fix '{api,src}/**/*{.ts,.tsx}'"
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-typescript": "7.7.7",
    "@babel/runtime": "7.7.7",
    "@skatteetaten/ci-scripts": "2.0.2",
    "@types/cors": "2.8.6",
    "@types/enzyme": "3.10.4",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/express": "4.17.2",
    "@types/graphql": "14.5.0",
    "@types/http-proxy-middleware": "0.19.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "24.0.25",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.17",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "7.1.5",
    "@types/react-router-dom": "5.1.3",
    "@types/react-select": "3.0.8",
    "@types/styled-components": "4.4.2",
    "@types/uuid": "3.4.6",
    "dotenv": "8.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-sonarjs": "0.5.0",
    "jest-junit": "10.0.0",
    "factory.ts": "0.5.1",
    "jest-each": "24.9.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react-scripts": "3.3.0",
    "redux-devtools-extension": "2.13.8",
    "rimraf": "3.0.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.4",
    "typescript-styled-plugin": "0.15.0",
    "webpack-cli": "3.3.10"
  },
  "jest-junit": {
    "suiteName": "Unit Tests",
    "outputDirectory": "coverage/surefire-reports",
    "outputName": "TEST-jest.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "proxy": "http://localhost:9090",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.d.ts",
      "!src/setupTests.ts",
      "!src/services/registerServiceWorker.ts"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
